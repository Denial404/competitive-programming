import java.io.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt(); 
        int k = in.nextInt();
        int sum = 0;
        
        for (int i = 0; i < k; i++) {
            sum += N * Math.pow(10, i+1);
        }
        System.out.println(N + sum);
    }
}
