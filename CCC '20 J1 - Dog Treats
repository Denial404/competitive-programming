import java.io.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int S = in.nextInt();
        int M = in.nextInt();
        int L = in.nextInt();
        int happy = 1*S+2*M+3*L;
        
        if (happy>=10) {
            System.out.println("happy");
        } else {
            System.out.println("sad");
        }
    }
}
